ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -DTORCH_EXTENSION_NAME=knn -DTORCH_API_INCLUDE_EXTENSION_H -isystem /home/zjunlict/miniconda3/envs/NRobo/lib/python3.6/site-packages/torch/include -isystem /home/zjunlict/miniconda3/envs/NRobo/lib/python3.6/site-packages/torch/include/torch/csrc/api/include -isystem /home/zjunlict/miniconda3/envs/NRobo/lib/python3.6/site-packages/torch/include/TH -isystem /home/zjunlict/miniconda3/envs/NRobo/lib/python3.6/site-packages/torch/include/THC -isystem /usr/local/cuda/include -isystem /home/zjunlict/miniconda3/envs/NRobo/include/python3.6m -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++11 -O2
cuda_flags = -DTORCH_EXTENSION_NAME=knn -DTORCH_API_INCLUDE_EXTENSION_H -isystem /home/zjunlict/miniconda3/envs/NRobo/lib/python3.6/site-packages/torch/include -isystem /home/zjunlict/miniconda3/envs/NRobo/lib/python3.6/site-packages/torch/include/torch/csrc/api/include -isystem /home/zjunlict/miniconda3/envs/NRobo/lib/python3.6/site-packages/torch/include/TH -isystem /home/zjunlict/miniconda3/envs/NRobo/lib/python3.6/site-packages/torch/include/THC -isystem /usr/local/cuda/include -isystem /home/zjunlict/miniconda3/envs/NRobo/include/python3.6m -D_GLIBCXX_USE_CXX11_ABI=0 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr -gencode=arch=compute_75,code=sm_75 --compiler-options '-fPIC' -DCUDA_HAS_FP16=1 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ -std=c++11
ldflags = -shared -L/usr/local/cuda/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out
  depfile = $out.d
  deps = gcc

rule cuda_compile
  command = $nvcc $cuda_flags -c $in -o $out

rule link
  command = $cxx $in $ldflags -o $out

build knn.o: compile /home/zjunlict/dhz/NeuroRobotics/REDE/lib/KNN_CUDA/knn_cuda/csrc/cuda/knn.cpp
build knn.cuda.o: cuda_compile /home/zjunlict/dhz/NeuroRobotics/REDE/lib/KNN_CUDA/knn_cuda/csrc/cuda/knn.cu

build knn.so: link knn.o knn.cuda.o

default knn.so

